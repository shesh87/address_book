<h1>My Address Book</h1>

<ul>
	<li><%= link_to("New Contact", add_contacts_path) %></li>
	<li><button class="test">Testing</button></li>
</ul>


<% ("A"..."Z").each do |letter| %>
	<%= link_to(letter, letter_sort_path(letter)) %>
<% end %>





<% @contacts.each do |person| %>
	<div style="margin-bottom: 10px;">
		<ul>
			<li><%= link_to("#{person.name}", contacts_id_path(person.id)) %></li>
		</ul>
	</div>
<% end %>



